		{
            try
            {
                ArrayList fileNamesArray = new ArrayList();
                string rutaArchivos = Dts.Variables["User::RutaArchivoEntradaCancelarPoliza"].Value.ToString();
                string[] files = Directory.GetFiles(rutaArchivos, "*.TXT");

                foreach (string file in files)
                {
                    string nombreSinExt = Path.GetFileNameWithoutExtension(file);

                    if (nombreSinExt.StartsWith("336CO23NS318") && nombreSinExt.EndsWith("00"))
                    {
                        fileNamesArray.Add(Path.GetFileName(file));
                    }
                    else
                        continue;
                }
                fileNamesArray.Sort();
